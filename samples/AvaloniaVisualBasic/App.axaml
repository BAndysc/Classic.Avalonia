<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:avaloniaVisualBasic="clr-namespace:AvaloniaVisualBasic"
             x:Class="AvaloniaVisualBasic.App"
             RequestedThemeVariant="{x:Static ClassicTheme.Classic}">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="Controls/MDIWindow.axaml" />
                <ResourceInclude Source="Controls/MDIHost.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

    <Application.Styles>
        <ClassicTheme />
        <StyleInclude Source="avares://Classic.Avalonia.Theme.Dock/Themes/DockFluentTheme.axaml" />
    </Application.Styles>

    <Application.DataTemplates>
        <avaloniaVisualBasic:ViewLocator />
    </Application.DataTemplates>
</Application>